<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.a2020frcscouter">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="glass.door.a2020frcscouter.MyAppy"
        android:allowBackup="true"
        android:icon="@mipmap/ic_trojan_robotics"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_trojan_robotics"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Launcher">
        <activity
            android:name="glass.door.a2020frcscouter.TeamInfoActivity"
            android:label="@string/title_activity_team_info"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="glass.door.a2020frcscouter.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.Launcher"
            >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>